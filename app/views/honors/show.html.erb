  <div class="form-all">
    <ul class="form-section page-section">
      <li id="cid_1" class="form-input-wide" data-type="control_head">
        <div class="form-header-group">
          <div class="header-text httal htvam">
            <h2 id="header_1" class="form-header">
              Review Extra Course
            </h2>
          </div>
        </div>
      </li>
      <li class="form-line" data-type="control_textbox" id="id_4">
        <label class="form-label form-label-left form-label-auto" id="label_4" for="input_4"> Course Id </label>
        <div id="cid_4" class="form-input jf-required">
         <%= @honor.course_id %>
        </div>
      </li>
      <li class="form-line" data-type="control_textbox" id="id_3">
        <label class="form-label form-label-left form-label-auto" id="label_3" for="input_3"> contract_id </label>
        <div id="cid_3" class="form-input jf-required">
         <%= @honor.contract_id %>
        </div>
      </li>
      <li class="form-line" data-type="control_textbox" id="id_5">
        <label class="form-label form-label-left form-label-auto" id="label_5" for="input_5"> Uid </label>
        <div id="cid_5" class="form-input jf-required">
         <%= @honor.uid %>
        </div>
      </li>
      <li class="form-line" data-type="control_dropdown" id="id_14">
        <label class="form-label form-label-left form-label-auto" id="label_14" for="input_14"> Semester </label>
        <div id="cid_14" class="form-input jf-required">
          <%= @honor.semester %>
        </div>
      </li>
      <li class="form-line" data-type="control_textbox" id="id_7">
        <label class="form-label form-label-left form-label-auto" id="label_7" for="input_7"> Year </label>
        <div id="cid_7" class="form-input jf-required">
          <%= @honor.year %>
        </div>
      </li>
      <li class="form-line" data-type="control_email" id="id_12">
        <label class="form-label form-label-left form-label-auto" id="label_12" for="input_12"> Professor mail id </label>
        <div id="cid_12" class="form-input jf-required">
           <%= @honor.prof_email %>
        </div>
      </li> 

      <li class="form-line" data-type="control_email" id="id_12">
        <label class="form-label form-label-left form-label-auto" id="label_12" for="input_12"> Grade </label>
        <div id="cid_12" class="form-input jf-required">
           <%= @honor.grade %>
        </div>
      </li>
 <li class="form-line" data-type="control_email" id="id_12">
